{
    "expo": {
        "name": "client",
        "slug": "client",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "backgroundColor": "#3A5C42",
        "primaryColor": "#3A5C42",
        "splash": {
            "backgroundColor": "#3A5C42"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#3A5C42"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": ["expo-router"],
        "experiments": {
            "tsconfigPaths": true,
            "typedRoutes": true
        }
    }
}
